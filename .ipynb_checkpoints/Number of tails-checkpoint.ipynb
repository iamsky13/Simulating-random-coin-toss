    "import numpy as np\n",
    "np.random.seed(123) \n",
    "tail = [0]\n",
    "for x in range(10) : #tossing 10 times\n",
    "    coin = np.random.randint(0,2) #0 means head and 1 means tail\n",
    "    tail.append(tail[x]+coin)\n",
    "    print(coin)\n",
    "    print(tail) \n",
    "print(\"\\n\")\n",
    "print(\"total no. of tails : \"+str(tail[-1]))"
   ]
